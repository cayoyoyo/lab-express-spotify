  <link rel="stylesheet" type="text/css" href="/styles/style.css">
  
  <h1>Tracks</h1>

  <ul>
    {{#each tracks}}
      <li>
        <h3>{{name}}</h3>
        <audio controls>
          <source src="{{preview_url}}" type="audio/mpeg">
          Your browser does not support the audio element.
        </audio>
      </li>
    {{/each}}
  </ul>